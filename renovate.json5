{
  "extends": [
    "config:base",
    "docker:disable",
    ":timezone(Asia/Tokyo)"
  ],
  "prConcurrentLimit": 0,
  "labels": ["Type:dependencies"],
  // npm向けの設定
  "npm": {
    "extends": [
      ":ignoreUnstable",
    ],
    "major": {
        "automerge": false
    },
    "minor": {
        "automerge": true
    },
    "patch": {
        "automerge": true
    },
    "pin": {
        "automerge": true
    },
  }
}
